<h1>New Attraction</h1>

<%= form_for(@attraction) do |f| %>
  <%= f.label :name %>
  <p><%= f.text_field :name %></p>

  <%= f.label :'Height Requirement' %>
  <p><%= f.text_field :min_height %></p>

  <%= f.label :happiness_rating %>
  <p><%= f.text_field :happiness_rating %></p>

  <%= f.label :nausea_rating %>
  <p><%= f.text_field :nausea_rating %></p>

  <%= f.label :tickets %>
  <p><%= f.text_field :tickets %></p>

  <%= f.submit %>
<% end %>
